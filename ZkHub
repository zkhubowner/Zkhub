local v007 = v062:CreateSection("Extra Combat Hacks")
local v008 = game:GetService("v008")
local v094 = game:GetService("v094")
local v095 = game:GetService("v095")
local v096 = v008.v055
local v097 = v096:GetMouse()
local v098 = workspace.CurrentCamera
_G.aimbotSystemEnabled = false
local v099 = 100
local v100 = false
local v101 = Color3.new(1, 1, 0) 
local v102 = nil
local v012 createAimCircle()
    if not v102 then
        v102 = Drawing.new("Circle")
        v102.Visible = false
        v102.Color = v101
        v102.Thickness = 2
        v102.Radius = v099
        v102.Filled = false
    end
end
local v012 destroyAimCircle()
    if v102 then
        v102:Remove()
        v102 = nil
    end
end
local v012 isPlayerInAimCircle(v002)
    local v103 = v002.Character
    if v103 and v103:FindFirstChild("HumanoidRootPart") then
        local v104 = v103.HumanoidRootPart
        local v105, onScreen = v098:WorldToScreenPoint(v104.Position)
        if onScreen then
            local v106 = Vector2.new(v097.X, v097.Y)
            local v107 = (v106 - Vector2.new(v105.X, v105.Y)).Magnitude
            return v107 <= v099, v107
        end
    end
    return false, math.huge
end
local v012 getClosestPlayer()
    local v108 = nil
    local v109 = v099
    for _, v002 in pairs(v008:GetPlayers()) do
        if v002 ~= v096 then
            local v103 = v002.Character
            if v103 and v103:FindFirstChild("Humanoid") and v103.Humanoid.Health > 0 then
                local v110, distance = isPlayerInAimCircle(v002)
                if v110 and distance < v109 then
                    v109 = distance
                    v108 = v002
                end
            end
        end
    end
    return v108
end
local v012 aimAtTarget(v002)
    local v103 = v002.Character
    if v103 and v103:FindFirstChild("HumanoidRootPart") and v103:FindFirstChild("Head") then
        local v111 = v103.Head
        v098.CFrame = CFrame.new(v098.CFrame.Position, v111.Position)
    end
end
v095.InputBegan:Connect(v012(input)
    if input.UserInputType == Enum.UserInputType.MouseButton2 and _G.aimbotSystemEnabled then
        v100 = true
    end
end)
v095.InputEnded:Connect(v012(input)
    if input.UserInputType == Enum.UserInputType.MouseButton2 then
        v100 = false
    end
end)
v094.RenderStepped:Connect(v012()
    if not _G.aimbotSystemEnabled then
        destroyAimCircle()
        return
    end
    if not v102 then
        createAimCircle()
    end
    v102.Position = Vector2.new(v097.X, v097.Y + 40)
    v102.Visible = true
    if v100 then
        local v108 = getClosestPlayer()
        if v108 then
            aimAtTarget(v108)
        end
    end
end)
local v024 = v062:CreateToggle({
    Name = "Aimbot",
    CurrentValue = false,
    Flag = "Aimbot",
    Callback = v012(Value)
        _G.aimbotSystemEnabled = Value
        if not _G.aimbotSystemEnabled then
            destroyAimCircle()
        end
    end
})
v006:CreateSection("Atm")
local v112 = 2000000
v006:CreateInput({
    Name = "Deposit",
    PlaceholderText = "Enter v113 to deposit",
    RemoveTextAfterFocusLost = true,
    Callback = v012(input)
        local v113 = tonumber(input)
        if v113 then
            v112 = v112 + v113
            game:GetService("v009"):WaitForChild("ATMRF"):InvokeServer("deposit", v113)
            v001:Notify({
                Title = "Deposit Complete",
                Content = "Deposited $" .. v113 .. "\nNew Balance: $" .. v112,
                Duration = 5
            })
        else
            v001:Notify({
                Title = "Error",
                Content = "Invalid deposit v113.",
                Duration = 4
            })
        end
    end
})
v006:CreateInput({
    Name = "Withdraw",
    PlaceholderText = "Enter v113 to withdraw",
    RemoveTextAfterFocusLost = true,
    Callback = v012(input)
        local v113 = tonumber(input)
        if v113 then
            if v113 <= v112 then
                v112 = v112 - v113
                game:GetService("v009"):WaitForChild("ATMRF"):InvokeServer("withdraw", v113)
                v001:Notify({
                    Title = "Withdraw Complete",
                    Content = "Withdrew $" .. v113 .. "\nNew Balance: $" .. v112,
                    Duration = 5
                })
            else
                v001:Notify({
                    Title = "Error",
                    Content = "Insufficient funds.",
                    Duration = 4
                })
            end
        else
            v001:Notify({
                Title = "Error",
                Content = "Invalid withdraw v113.",
                Duration = 4
            })
        end
    end
})
v006:CreateButton({
    Name = "Check Balance",
    Callback = v012()
        v001:Notify({
            Title = "Bank Balance",
            Content = "Current Balance: $" .. v112,
            Duration = 5
        })
    end
})
