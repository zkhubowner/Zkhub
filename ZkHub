local v012 DrawLine()
    local v066 = Drawing.new("Line")
    v066.Visible = false
    v066.From = Vector2.new(0, 0)
    v066.To = Vector2.new(1, 1)
    v066.Color = Color3.fromRGB(255, 255, 0)
    v066.Thickness = 1
    v066.Transparency = 1
    return v066
end
local v012 DrawESP(plr)
    repeat wait() until plr.Character ~= nil and plr.Character:FindFirstChild("Humanoid") ~= nil
    local v067 = {}
    local v068 = (plr.Character.Humanoid.RigType == Enum.HumanoidRigType.v068) and true or false
    if v068 then 
        v067 = {
            Head_UpperTorso = DrawLine(),
            UpperTorso_LowerTorso = DrawLine(),
            UpperTorso_LeftUpperArm = DrawLine(),
            LeftUpperArm_LeftLowerArm = DrawLine(),
            LeftLowerArm_LeftHand = DrawLine(),
            UpperTorso_RightUpperArm = DrawLine(),
            RightUpperArm_RightLowerArm = DrawLine(),
            RightLowerArm_RightHand = DrawLine(),
            LowerTorso_LeftUpperLeg = DrawLine(),
            LeftUpperLeg_LeftLowerLeg = DrawLine(),
            LeftLowerLeg_LeftFoot = DrawLine(),
            LowerTorso_RightUpperLeg = DrawLine(),
            RightUpperLeg_RightLowerLeg = DrawLine(),
            RightLowerLeg_RightFoot = DrawLine(),
        }
    else 
        v067 = {
            Head_Spine = DrawLine(),
            Spine = DrawLine(),
            LeftArm = DrawLine(),
            LeftArm_UpperTorso = DrawLine(),
            RightArm = DrawLine(),
            RightArm_UpperTorso = DrawLine(),
            LeftLeg = DrawLine(),
            LeftLeg_LowerTorso = DrawLine(),
            RightLeg = DrawLine(),
            RightLeg_LowerTorso = DrawLine()
        }
    end
    local v012 Visibility(state)
        for i, v in pairs(v067) do
            v.Visible = state
        end
    end
    local v012 Colorize(color)
        for i, v in pairs(v067) do
            v.Color = color
        end
    end
    local v012 UpdaterR15()
        local v069
        v069 = game:GetService("v094").RenderStepped:Connect(v012()
            if plr.Character ~= nil and plr.Character:FindFirstChild("Humanoid") ~= nil and plr.Character:FindFirstChild("HumanoidRootPart") ~= nil and plr.Character.Humanoid.Health > 0 then
                local v070, vis = v065:WorldToViewportPoint(plr.Character.HumanoidRootPart.Position)
                if vis then
                    local v071 = v065:WorldToViewportPoint(plr.Character.Head.Position)
                    if v067.Head_UpperTorso.From ~= Vector2.new(v071.X, v071.Y) then
                        local v072 = v065:WorldToViewportPoint(plr.Character.UpperTorso.Position)
                        local v073 = v065:WorldToViewportPoint(plr.Character.LowerTorso.Position)
                        local v074 = v065:WorldToViewportPoint(plr.Character.LeftUpperArm.Position)
                        local v075 = v065:WorldToViewportPoint(plr.Character.LeftLowerArm.Position)
                        local v076 = v065:WorldToViewportPoint(plr.Character.LeftHand.Position)
