-- Get HWID (if executor supports it)
local hwid = "Unknown"
local hwidPart1 = "Unknown"
local hwidPart2 = "Unknown"

if syn and syn.get_hwid then
    hwid = syn.get_hwid()  -- Synapse X HWID method
elseif gethwid then
    hwid = gethwid()  -- For executors that use this method
elseif fluxus and fluxus.get_hwid then
    hwid = fluxus.get_hwid()  -- Fluxus HWID method
end

-- Split HWID into two parts (for example, split at character 32)
-- You can adjust the splitting logic based on how you want to format it.
if hwid ~= "Unknown" then
    hwidPart1 = hwid:sub(1, #hwid // 2)  -- First half of the HWID
    hwidPart2 = hwid:sub(#hwid // 2 + 1)  -- Second half of the HWID
    hwid = hwidPart1 .. " - " .. hwidPart2  -- Format with dash
end

-- Basic info
local executor = "Unknown"
if syn then
    executor = "Synapse X"
elseif secure_load then
    executor = "Script-Ware"
elseif KRNL_LOADED then
    executor = "KRNL"
elseif is_sirhurt_closure then
    executor = "SirHurt"
elseif pebc_execute then
    executor = "ProtoSmasher"
elseif fluxus then
    executor = "Fluxus"
elseif identifyexecutor then
    executor = identifyexecutor()
elseif getexecutorname then
    executor = getexecutorname()
end

